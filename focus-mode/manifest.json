{
    "manifest_version": 3,
    "name": "Focus Mode",
    "description": "Not sure",
    "version":"3.0",
    "icons": {
        "16": "images/hi_16.png",
        "128": "images/reading.png"
    },
    "action": {
        "default_icon": {
            "16": "images/hi_16.png",
            "128": "images/reading.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    }
}